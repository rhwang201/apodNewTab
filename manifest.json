{
  "manifest_version": 2,
  "name": "APOD New Tab",
  "short_name": "APOD New Tab",
  "version": "1.3",

  "description": "See pictures from Astronomy Picture of the Day when you open a new tab!",
  "chrome_url_overrides": {
    "newtab": "apod.html"
  },
  "icons": {
    "48": "img/48.png"
  },
  "permissions": [
    "http://apod.nasa.gov/",
    "http://api.data.gov/",
    "https://api.data.gov/"
  ]
}
